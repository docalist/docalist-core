{
	"type" : "wordpress-plugin",
	"name" : "docalist/docalist-core",
	"description" : "Docalist : socle de base.",
	"homepage" : "https://docalist.org/",
	"license" : "GPL-2.0-or-later",
	"authors" : [{
			"name" : "Daniel MÃ©nard",
			"email" : "daniel.menard@laposte.net",
			"homepage" : "https://docalist.org/"
		}
	],
	"keywords" : [
		"docalist"
	],
	"support" : {
		"issues" : "https://github.com/docalist/docalist/issues",
		"forum" : "https://groups.google.com/forum/#!forum/docalist-discuss"
	},
	"require" : {
		"php": ">=7.2",
		"composer/installers": "~1.0",
		"monolog/monolog": "~1.23",
		"psr/log": "~1.0",
		"symfony/http-foundation": "^6.2"
	},
	"autoload" : {
		"psr-4" : {
			"Docalist\\" : "class"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Docalist\\Tests\\" : "tests/Docalist"
		}
	},
	"config" : {
		"sort-packages" : true,
		"optimize-autoloader" : true,
		"classmap-authoritative" : true
	},
    "extra" : {
        "branch-alias" : {
            "dev-master" : "3.4-dev"
        }
    }
}
